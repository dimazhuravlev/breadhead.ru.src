{
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@site": "./"
        }
      }
    ]
  ],
  "env": {
    "development": {
      "presets": [
        [ "next/babel" ]
      ]
    },
    "production": {
      "presets": [
        [ "next/babel" ]
      ]
    },
    "test": {
      "presets": ["env", "react"]
    }
  }
}
